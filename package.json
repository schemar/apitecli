    "convict": "^6.0.0",
    "http-status-codes": "^1.4.0",
    "import-jsx": "^4.0.0",
    "ink": "^2.7.1",
    "ink-box": "^1.0.0",
    "ink-spinner": "^3.0.1",
    "ink-text-input": "^3.2.2",
    "ink-use-stdout-dimensions": "^1.0.5",
    "markdown-table": "^2.0.0",
    "prop-types": "^15.7.2",
